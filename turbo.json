{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "lint": {
            "dependsOn": [
                "^lint"
            ]
        },
        "typecheck": {
            "dependsOn": [
                "^typecheck"
            ]
        },
        "clean": {
            "outputs": []
        },
        "docs": {
            "outputs": [
                "build/**"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "test": {
            "dependsOn": [
                "^test"
            ]
        }
    }
}