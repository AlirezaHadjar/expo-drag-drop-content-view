{
    "name": "@alirezahadjar/monorepo",
    "private": true,
    "version": "0.5.0",
    "scripts": {
        "build": "turbo build",
        "clean": "turbo clean && rm -rf node_modules",
        "docs": "turbo docs --filter=docs",
        "dev": "turbo dev",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "lint": "turbo lint",
        "typecheck": "turbo typecheck",
        "test": "turbo test"
    },
    "dependencies": {
        "@changesets/cli": "^2.27.1",
        "@manypkg/cli": "^0.21.2",
        "@typescript-eslint/typescript-estree": "^7.1.1",
        "eslint-plugin-import": "^2.29.1",
        "prettier": "^3.2.5",
        "react-loadable": "npm:@docusaurus/react-loadable@5.5.2",
        "turbo": "^1.12.5",
        "typescript": "^5.4.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AlirezaHadjar/expo-drag-drop-content-view.git"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "packageManager": "yarn@4.1.1"
}